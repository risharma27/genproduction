import model {model}
define L = lp lp~
define N = vlp vlp~
define boson_L = z h
define boson_N = w+ w-
define lepton = {decay_lepton}

generate p p > L L, (L > boson_L lepton)
add process p p > N N, (N > boson_N lepton)
add process p p > L N, (L > boson_L lepton), (N > boson_N lepton)

output {output_name} -nojpeg
